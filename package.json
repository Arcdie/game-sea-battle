{
  "name": "Game.Sea.Battle",
  "version": "1.0.0",
  "description": "Game sea battle",
  "main": "app.js",
  "scripts": {
    "start": "node backend/app",
    "dev": "nodemon backend/app",
    "dev-ts": "nodemon --watch 'backend/**/*.ts' --exec 'ts-node' backend/app.ts",
    "build-css": "node-sass --include-path scss frontend/scss/styles.scss   frontend/stylesheets/styles.css",
    "watcher": "nodemon -e scss -x \"npm run build-css\""
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "redis": "^3.1.2",
    "serve-favicon": "^2.5.0",
    "validator": "^10.11.0",
    "winston": "^3.2.1",
    "ws": "^8.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "^4.6.2",
    "node-sass": "^6.0.1"
  }
}
